# Vector AI API Configuration (Required)
VECTOR_API_KEY=your_vector_ai_api_key_here
VECTOR_API_URL=https://api.vector-ai.pro/v1

# Blockchain APIs (Required)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here
ARBISCAN_API_KEY=your_arbiscan_api_key_here
BASESCAN_API_KEY=your_basescan_api_key_here
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Code Analysis APIs (Optional)
GITHUB_TOKEN=your_github_token_here
SOURCIFY_API_URL=https://sourcify.dev/server

# Security & Audit APIs (Optional)
CERTIK_API_KEY=your_certik_api_key_here
CONSENSYS_API_KEY=your_consensys_api_key_here
IMMUNEFI_API_KEY=your_immunefi_api_key_here

# Application Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Analysis Settings
ANALYSIS_DEPTH=comprehensive
SECURITY_THRESHOLD=7.0
CACHE_DURATION=3600000
MAX_CONCURRENT_ANALYSIS=5

# AI Model Configuration
AI_MODEL=vgpt-premium
AI_TEMPERATURE=0.3
AI_MAX_TOKENS=4000

# Database (Optional - for caching and history)
DATABASE_URL=postgresql://user:password@localhost:5432/contract_analyzer
REDIS_URL=redis://localhost:6379

# Monitoring & Alerts (Optional)
DISCORD_WEBHOOK_URL=your_discord_webhook_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
SLACK_WEBHOOK_URL=your_slack_webhook_here

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=900000

# File Storage (Optional)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_S3_BUCKET=your_s3_bucket_here
AWS_REGION=us-east-1 